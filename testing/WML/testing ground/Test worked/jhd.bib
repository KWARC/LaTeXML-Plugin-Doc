@Article{ RecioVelez1999,
  title = "{Automatic discovery of theorems in elementary geometry}",
  author = {Recio,T. and V\'elez,M.P.},
  journal = {J. Automat. Reason.},
  volume = {23},
  year = {1999},
  pages = {63-82},
  annote = {This article adds to the methods available for automated reasoning in
geometry using polynomial algorithms, here Grobner bases. In particular an
approach to theorem discovery is considered, which goes beyond the
discovery of non-degeneracy conditions representing negative hypotheses to
the discovery of new positive hypotheses needed to guarantee the generic
truth of a theorem. If a theorem as stated is not generically true, the
approach calculates the elimination ideal in the independent variables of
the hypotheses plus the conclusion (rather than the "negation" of the 
conclusion as in the test for generic truth). The resulting basis, if
non-zero, gives new potential hypotheses to add to the existing ones in
order to perhaps make the theorem generically true after modification of
the set of independent variables. The method is not complete in the absence
of factorization methods but can provide useful information even if the new
hypotheses prove insufficient for generic truth. Examples are given to
illustrate the procedure.},
  keywords = {GEOM PROVE}
}

