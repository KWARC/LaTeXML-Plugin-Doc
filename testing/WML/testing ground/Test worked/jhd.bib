@Article{ Abbottetal2000b,
  title = "{Computing Ideals of Points}",
  author = {Abbott,J.A. and Bigatti,A. and Kreuzer,M. and Robbiano,L.},
  journal = {J. Symbolic Comp.},
  volume = {30},
  year = {2000},
  pages = {341-356},
  annote = {refers to Wang, Guy, Davenport
The advent of computational algebra has, to some extent, added a flavor of
experimental science to the theoretical fields of commutative algebra and
algebraic geometry, allowing for the production of very complicated (or
sometimes incredibly simple) examples. This incredibly useful tool is
founded not only on the use of faster and faster machines, but also on the
development of more and more efficient algorithms. The current paper
considers the problem of computing the ideal of a finite set of points in
affine or projective space. A modular version of the Buchberger-Moller
algorithm is produced, and its complexity studied. In the projective case a
main difficulty in general is to find an efficient stopping criterion, and
this is done in this paper. These algorithms are implemented in the
computer program CoCoA, and some experimental timings are given at the end
of the paper.},
  keywords = {GROBNER}
}

