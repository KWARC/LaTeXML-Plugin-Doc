@Article{ Sachkov1996,
  title = "{Ideals and filters of partitions and cyclic classes, and invariance domains  of permutations}",
  author = {Sachkov,V.N.},
  journal = {Diskret. Mat.},
  volume = {8},
  year = {1996},
  pages = {3-21},
  annote = {Let $P(n,k)$ be the probability that a random permutation of the set
$\{1,2,\cdots,n\}$ has an invariant $k$-set. There are well-known
evaluations for $P(n,k)$ as follows: $P(n,k)\leq {2\over 3}$
[J. D. Dixon, Discrete Math. 105 (1992), no. 1-3, 25--39; MR 93e:20004] 
and $P(n,k)\leq A/{k\sp {100}},$ where $A$ is a constant
[T. Luczak and L. Pyber, Combin. Probab. Comput. 2 (1993), no. 4, 505--512; MR 95b:20004]. 
Considering ideals and filters in the ordered set of all infinite
sequences of integers the author describes the distribution
$\zeta\sb {nk}$ determined by the following equation: $$P\{\zeta\sb
{nk}=r\}=\sum\sb {\nu=r}\sp {R\sb k}(-1)\sp {\nu-r}{{\nu}\choose{r}} B\sb
{\nu}(n,k), \quad r=0,1,\cdots,R\sb k,$$ where $R\sb k$ is the number of
partitions of the integer $k$ and $B\sb{\nu}(n,k)$ are binomial moments of
$\zeta\sb {nk}$ for $\nu=0,1,\cdots,R\sb k.$ In particular the author
obtains $$P\{\zeta\sb {nk}\geq 1\}=\sum\sb {\nu=1}\sp {R\sb k}(-1)\sp
{\nu-1}B\sb {\nu}(n,k).$$ The main result consists of the following
equation for $P(n,k)$: $$P(n,k)=P\{\zeta\sb {nk}\geq 1\}=\sum\sb
{\nu=1}\sp {R\sb k}(-1)\sp {\nu-1}B\sb{\nu}(n,k).$$},
  keywords = {GROUP}
}

