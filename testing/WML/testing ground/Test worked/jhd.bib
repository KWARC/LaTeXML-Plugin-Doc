@Article{ Mansfield2001,
  title = "{Algorithms for symmetric differential systems}",
  author = {Mansfield,E.L.},
  journal = {Found. Comput. Math.},
  volume = {1},
  year = {2001},
  pages = {335-383},
  annote = {A fundamental operation in the study of overdetermined systems of partial
differential equations is the completion of such systems, i.e., the
addition of all integrability conditions. There exist a number of different
approaches to this problem; here a differential algebraic version with the
Kolchin-Ritt algorithm as basis is employed. This is a simple critical pair
completion algorithm analogous to the Buchberger algorithm for determining
Grobner bases. The required computations tend to be rather tedious and
sometimes blow up, so that they cannot be completed even with a computer
algebra system. 
A special situation appears when the system to be completed is invariant
under the action of a symmetry group. The algebraic completion requires the
choice of a ranking for the derivatives which typically breaks the
symmetry and increases the expression swell. In order to avoid this
problem, the author rewrites the system in terms of differential invariants
using the recently developed moving-frame method of M.E.Fels and P.J.Olver
[Acta Appl. Math. 51 (1998), no. 2, 161--213; MR 99c:53012; Acta Appl.
Math. 55 (1999), no. 2, 127--208; MR 2000h:58024]. She formulates a variant
of the Kolchin-Ritt algorithm applicable in this situation and uses it for
the analysis of the $(2+1)$-dimensional d'Alembert-Hamilton system. 
This adaption of the Kolchin-Ritt algorithm is nontrivial, as the invariant
differential operators associated with the moving frames do not commute in
general. It is described for systems with at most polynomial
nonlinearities. Furthermore, it is assumed that the action of the Lie
algebra is of rational type and the moving frame is of polynomial type in
order to be able to perform effectively all computations. Finally, a brief
description of a Maple implementation and a comparison with earlier work by
I. Lisle ["Equivalence transformations for classes of differential
equations", Ph.D. thesis, Univ. British Columbia, 1992] is given. 
The examples given demonstrate the high potential of the approach; the
expressions involved become significantly smaller and are much easier to
handle. A caveat is that it relies on several plausible but yet
unproven conjectures of the author.},
  keywords = {PDE}
}

