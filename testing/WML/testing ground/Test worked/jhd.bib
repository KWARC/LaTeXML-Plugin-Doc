@Article{ Mourrain1999,
  title = "{A new criterion for normal form algorithms}",
  author = {Mourrain,B.},
  journal = {Applied algebra},
  year = {1999},
  pages = {430--443},
  annote = {Let $R = k[x\sb 1,\dots, x\sb n]$ be a polynomial ring and let $I\subseteq 
R$ be an ideal. The author wants to give an algorithm for computing normal
forms of the elements of the quotient ring $R/I$ not directly based on
Grobner basis computation. The basic idea followed is to construct a
suitable $k$-vector space $B \subseteq R$ such that $R = I \oplus B$. As
soon as this decomposition is found, then the projection of $R$ onto $B$
gives the normal forms of the quotient. 
The given criterion does not require any monomial ordering and generalizes
the Buchberger criterion of $S$-polynomials. 
The author considers in particular the case in which $R/I$ is
zero-dimensional and successively he shows that
his construction naturally generalizes to the case of Laurent polynomials.},
  keywords = {GROBNER}
}

