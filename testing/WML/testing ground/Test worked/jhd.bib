@Misc{ GiesbrechtRoche2008,
  title = "{Interpolation of Shifted-Lacunary Polynomials}",
  author = {Giesbrecht,M. and Roche,D.S.},
  year = {2008},
  howpublished = {http://arxiv.org/abs/0810.5685},
  annote = {Given a "black box" function to evaluate an unknown rational polynomial f in Q[x] at points modulo a prime p, we exhibit algorithms to compute the representation of the polynomial in the sparsest shifted power basis. That is, we determine the sparsity t, the shift alpha, the exponents 0<=e1<e2<...<et, and the coefficients c1,...,ct in Q\{0} such that f=c1*(x-alpha)^e1+c2*(x-alpha)^e2+...+ct*(x-alpha)^et. The computed sparsity t is absolutely minimal over any shifted power basis. The novelty of our algorithm is that the complexity is polynomial in the (sparse) representation size, log(alpha)+ sum_i(log|c_i|+log(e_i)) and in particular is logarithmic in deg(f). Our method combines previous celebrated results on sparse interpolation and computing sparsest shifts, and provides a way to handle polynomials with extremely high degree which are, in some sense, sparse in information.},
  keywords = {POLY SPARSE}
}

@Misc{ GiesbrechtRoche2009,
  title = "{Detecting lacunary perfect powers and computing their roots}",
  author = {Giesbrecht,M. and Roche,D.S.},
  year = {2009},
  howpublished = {http://arxiv.org/abs/0901.1848},
  annote = {We consider the problem of determining whether a lacunary (also called a sparse or super-sparse) polynomial f is a perfect power, that is, f=h^r for some other polynomial h and r, and of finding h and r should they exist. We show how to determine if f is a perfect power in time polynomial in the number of non-zero terms of f, and in terms of log(deg(f)), i.e., polynomial in the size of the lacunary representation. The algorithm works over GF(q)[x] (for large characteristic) and over Z[x], where the cost is also polynomial in log||f||. We also give a Monte Carlo algorithm to find h if it exists, for which our proposed algorithm requires polynomial time in the output size, i.e., the sparsity and height of h. Conjectures of Erdos and Schinzel, and recent work of Zannier, suggest that h must be sparse. Subject to a slightly stronger conjectures we give an extremely efficient algorithm to find h via a form of sparse Newton iteration. We demonstrate the efficiency of these algorithms with an implementation using the C++ library NTL.},
  keywords = {POLY SPARSE}
}

