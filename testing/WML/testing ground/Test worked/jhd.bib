@Article{ Latyshev2000,
  title = "{A combinatorial complexity of Gr\"obner bases}",
  author = {Latyshev,V.N.},
  journal = {J. Math. Sci. (New York)},
  volume = {102},
  year = {2000},
  pages = {4134-4138},
  annote = {Consider an ideal $I$ generated by elements of degree not exceeding $d$ in
a polynomial ring $k[x\sb 1,\dots,x\sb n]$. The author proves that there
exists an upper bound for the number of elements in the reduced Grobner
basis of $I$, that depends on $n$ and $d$ only.
The proofs are constructive. The author remarks that recurrence relations
for these bounds can be obtained from the proofs, but does not give them
explicitly.
Given generators of $I$, the standard Buchberger algorithm yields a
sequence of generators of $I$, a subset of which is the reduced Grobner
basis. The main observation is that the degrees of the elements in the
sequence grow in a controlled fashion, by at most a factor 2 at a time.
This fact is exploited and leads to the desired bound on the sequence
length.},
  keywords = {GROBNER COMPLEX}
}

