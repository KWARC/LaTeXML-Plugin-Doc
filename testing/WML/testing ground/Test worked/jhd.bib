@Article{ KoppenhagenMayr2001,
  title = "{An Optimal Algorithm for Constructing the Reduced Grobner Basis of  Binomial Ideals, and Applications to Commutative Semigroups J. Symbolic Comp. 31(2001) pp. 259-276}",
  author = {Koppenhagen,U. and Mayr,E.W.},
  journal = {MR 2001m:13050},
  year = {2001},
  annote = {The computation of Grobner bases is one of the fundamental algorithmic
problems in (commutative) algebra and algebraic geometry. Therefore, the
complexity analysis of the general problem of computing Grobner bases as
well as of concrete algorithms, e.g., Buchberger's algorithm, is of high
interest.
The central result of this paper is the presentation of an exponential
space algorithm for the computation of the reduced Grobner basis of a
multivariate binomial ideal. In consideration of Mayr&Meyer, Huynh, the
algorithm is space optimal. Moreover, it surpasses a previous result due
to the second author and K. Kuhnle [in Proceedings of the 1996
International Symposium on Symbolic and Algebraic Computation, ISSAC '96
(Zurich, 1996), 62--71, ACM, New York, 1996; Zbl 928.68058], who gave an
exponential space algorithm which, however, involves rather complex
parallel computations and makes extensive use of the parallel computation
assumption.
As an application of the above result the authors derive space optimal
decision procedures for the finite enumeration and subword problems for
commutative semigroups.},
  keywords = {GROBNER}
}

