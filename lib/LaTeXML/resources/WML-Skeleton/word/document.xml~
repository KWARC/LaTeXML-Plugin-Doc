            <w:t>6</w:t>
          </w:r>
          <w:r>
            <w:rPr/>
            <w:t>]</w:t>
          </w:r>
          <w:r w:rsidR="00597C89">
            <w:fldChar w:fldCharType="end"/>
          </w:r>
        </w:sdtContent>
      </w:sdt>
      <w:r>
        <w:rPr/>
        <w:t> and mechanized by the authors in </w:t>
      </w:r>
      <w:sdt>
        <w:sdtPr>
          <w:id w:val="4"/>
          <w:citation/>
        </w:sdtPr>
        <w:sdtEndPr/>
        <w:sdtContent>
          <w:r w:rsidR="00597C89">
